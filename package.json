  "bugs": {
    "url": "https://github.com/firstandthird/carousel/issues"
  },
  "homepage": "https://github.com/firstandthird/carousel#readme",
  "devDependencies": {
    "eslint-config-firstandthird": "^4.4.0",
    "eslint-plugin-import": "^2.18.0",
    "scriptkit": "^3.0.0"
  },
  "scriptkit": {
    "test": {
      "enabled": false
    },
